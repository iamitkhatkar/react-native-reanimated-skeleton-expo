{
  "name": "@rnrs/app",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "qa": "yarn format; yarn lint; yarn tsc",
    "format": "prettier --write --list-different '**/*.{json,ts,tsx}' --loglevel warn",
    "lint": "eslint . --fix --ext .ts,.tsx --no-cache",
    "align-deps": "yarn rnx-align-deps",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "type:check": "yarn tsc --noEmit"
  },
  "types": "../npm/react-native-reanimated-skeleton.d.ts",
  "dependencies": {
    "react": "18.2.0",
    "react-native": "^0.72.0",
    "expo-linear-gradient": "~12.3.0",
    "react-native-reanimated": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-decorators": "^7.22.15",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-transform-arrow-functions": "^7.22.5",
    "@babel/plugin-transform-private-methods": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.22.15",
    "@babel/runtime": "^7.22.15",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.0",
    "@rnx-kit/align-deps": "^2.2.4",
    "@rnx-kit/metro-config": "^1.3.9",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.3.0",
    "babel-jest": "^29.6.4",
    "babel-loader": "9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "jest": "^29.2.1",
    "metro-config": "^0.76.5",
    "metro-react-native-babel-preset": "^0.76.5",
    "react-test-renderer": "18.2.0"
  },
  "engines": {
    "node": ">=16"
  },
  "rnx-kit": {
    "kitType": "app",
    "alignDeps": {
      "requirements": [
        "react-native@0.72"
      ],
      "capabilities": [
        "animation",
        "babel-preset-react-native",
        "core",
        "core-android",
        "core-ios",
        "jest",
        "metro-config",
        "react",
        "react-test-renderer"
      ]
    }
  }
}
